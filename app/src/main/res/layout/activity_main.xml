                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="8dp"
                            android:layout_weight="50"
                            android:layout_marginBottom="10dp"
                            app:cardCornerRadius="16dp">
                            <RelativeLayout
